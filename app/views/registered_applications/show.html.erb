<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>

<h2><%= @registered_application.name %></h2>
<%= @registered_application.url %>
<%= link_to "Edit", edit_registered_application_path, class: 'btn btn-success' %>

<h4>Events <span class="badge"><%= @events.count %></span></h4>
<% @events.each do |e| %>
	<div>
		<%= e.name %>
	</div>
<% end %>

<%= pie_chart @registered_application.events.group(:name).count %>